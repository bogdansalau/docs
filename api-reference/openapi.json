  "paths": {
    "/cutlists": {
      "get": {
        "description": "Get cutlists",
        "operationId": "getCutlists",
        "responses": {
          "200": {
            "description": "Cutlists, each having an authorized link to a view-only page",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "format": "regex",
                            "pattern": "^[0-9A-Z]{26}$",
                            "description": "Unique identifier",
                            "example": "01J2ZPSXQ17G6QSSDJE599T0E2"
                          },
                          "name": {
                            "type": "string"
                          },
                          "requestType": {
                            "enum": [
                              "place_order",
                              "request_quote"
                            ]
                          },
                          "createdAt": {
                            "type": "number"
                          },
                          "updatedAt": {
                            "type": "number"
                          },
                          "submittedAt": {
                            "type": "number"
                          },
                          "cancelledAt": {
                            "type": "number"
                          },
                          "fullname": {
                            "type": "string"
                          },
                          "email": {
                            "type": "string"
                          },
                          "url": {
                            "type": "string",
                            "format": "url"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
